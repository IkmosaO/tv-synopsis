html
    head 
        body
            h1= showdata.title
                h2 Developed By 
                ul 
                    for developer in showdata.developedBy
                        li= developer
                h2 Starring 
                ul 
                    for star in showdata.starring 
                        li= star
                h2 Seasons
                ul
                    for showSeason in showdata.seasons
                        li 
                            div 
                                a(href=`/seasonpage/${showSeason.number}`)=`Season ${showSeason.number}` 